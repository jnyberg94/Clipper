{
  "rules": [
    {
      "name": "Skip images",
      "type": "fileExtension",
      "extensions": [".jpg", ".jpeg", ".png", ".gif", ".webp", ".heic"],
      "action": "skip",
      "reason": "Image file"
    }
  ],
  "settings": {
    "tmpFolder": "~/Library/Application Support/Clipper/tmp",
    "autoClearTmp": true,
    "permaSkip": [".DS_Store"],
    "videoProcessing": {
      "minFps": 50.01,
      "maxFps": 59.93,
      "targetFps": 60
    }
  }
}